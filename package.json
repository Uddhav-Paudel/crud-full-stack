{
  "name": "crud-full-stack",
  "version": "1.0.0",
  "description": "crud full stack application",
  "workspaces": [
    "fsapp",
    "backend"
  ],
  "scripts": {
    "lint": "npm run lint --workspaces",
    "test": "npm run test --workspaces",
    "build": "npm --workspace fsapp run build",
    "start:fsapp": "npm --workspace fsapp start",
    "preview": "npm --workspace fsapp run preview",
    "start:backend": "npm --workspace backend start"
  },
  "lint-staged": {
    "fsapp/**/*.{js,jsx}": "eslint --fix",
    "backend/**/*.js": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Uddhav-Paudel/crud-full-stack.git"
  },
  "keywords": [
    "full",
    "stack",
    "application"
  ],
  "author": "Uddhav",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Uddhav-Paudel/crud-full-stack/issues"
  },
  "homepage": "https://github.com/Uddhav-Paudel/crud-full-stack#readme",
  "devDependencies": {
    "globals": "^16.0.0",
    "husky": "^8.0.0"
  }
}
